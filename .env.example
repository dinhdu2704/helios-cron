# 必需配置
TARGET_CONTRACT=
PRIVATE_KEY=

# 网络配置（可选）
RPC_URL=https://testnet1.helioschainlabs.org

# Cron 任务配置（可选）
FREQUENCY=300                # 执行频率（区块数）
GAS_LIMIT=300000            # Gas 限制
GAS_PRICE=2                 # Gas 价格（gwei）
DEPOSIT=0.02                # 预存金额（HLS）
VALIDITY_WEEKS=2            # 有效期（周）

# 重试配置（可选）
MAX_RETRIES=3               # 最大重试次数
RETRY_DELAY=2000            # 重试延迟（毫秒）
